#compdef tw

local ret=1
local -a context line state state_descr alts opts
local -A opts_args

_arguments -A '-*' : \
    '(* -d --date)'{-d+,--date=}'[specify an exact date to timewarp to]:date' \
    '(-c --cmd)'{-c+,--cmd=}'[specify a command to run under a timewarp]:cmd' \
    '(- --)'{-,--}'[stop parsing options]' \
&& ret=0

return ret
