[Appearance]
AntiAliasFonts=true
BoldIntense=true
ColorScheme=OneDark
Font=JetBrainsMono Nerd Font Mono,10,-1,5,50,0,0,0,0,0
LineSpacing=0
UseFontLineChararacters=false

[Cursor Options]
CustomCursorColor=171,178,191
UseCustomCursorColor=false

[Encoding Options]
DefaultEncoding=UTF-8

[General]
Command=/usr/bin/zsh
Environment=COLORTERM=truecolor,TERM_PROGRAM=Konsole
LocalTabTitleFormat=%w
Name=zsh
Parent=FALLBACK/
RemoteTabTitleFormat=%h : %w
TerminalCenter=true
TerminalColumns=120
TerminalMargin=0
TerminalRows=35

[Keyboard]
KeyBindings=default

[Scrolling]
HistoryMode=1
HistorySize=2000
ScrollBarPosition=2
